<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="no_risk_data_tree" model="ir.ui.view">
            <field name="name">no.risk.data</field>
            <field name="model">no.risk.data</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="无风险收益率">
                    <field name='code' />
                    <field name='name' />
                    <field name='types' />
                    <field name='interest_rate' />
                    <field name='last_transaction_date' />
                    <field name='transaction_date_config_id' />
                    <field name='remark' />
                </tree>
            </field>
        </record>
        <record id="no_risk_data_form" model="ir.ui.view">
            <field name="name">no.risk.data</field>
            <field name="model">no.risk.data</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string="无风险收益率">
                    <sheet string="无风险收益率">
                        <group>
                            <group>
                                <field name="code"/>
                                <field name="transaction_date_config_id"/>
                            </group>
                            <group>
                                <field name="name"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="日收益率" >
                                <field name="no_risk_data_lines">
                                    <tree editable="bottom" default_order="transaction_date desc">
                                        <field name="transaction_date" />
                                        <field name="interest_rate" />
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
				</form>
            </field>
        </record>

    </data>
</odoo>