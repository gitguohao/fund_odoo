<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="fund_base_data_tree" model="ir.ui.view">
            <field name="name">fund_base_data</field>
            <field name="model">fund.base.data</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="交易日管理">
                    <field name='code' />
                    <field name='name' />
                    <field name='last_price' />
                    <field name='last_price_date' />
                    <field name='unit_net' />
                    <field name='total_net' />
                    <field name='types' />
                    <field name='establish_date' />
                    <field name='fund_manager' />
                    <field name='manager' />
                </tree>
            </field>
        </record>
        <record id="fund_base_data_form" model="ir.ui.view">
            <field name="name">fund.base.data</field>
            <field name="model">fund.base.data</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
				<form string="交易日管理">
                    <sheet string="交易日管理">
                        <group>
                            <group>
                                <field name="code"/>
                                <field name="types"/>
                                <field name="fund_manager"/>
                            </group>
                            <group>
                                <field name="name"/>
                                <field name="establish_date"/>
                                <field name="manager"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="日净值" >
                                <field name="fund_base_day_net_ids">
                                    <tree editable="bottom">
                                        <field name="dates" />
                                        <field name="beg_price" />
                                        <field name="end_price" />
                                        <field name="unit_net" />
                                        <field name="total_net" />
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
				</form>
            </field>
        </record>

    </data>
</odoo>