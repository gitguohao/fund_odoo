<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="market_config_tree" model="ir.ui.view">
            <field name="name">market.config</field>
            <field name="model">market.config</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="交易日管理" editable="bottom">
                    <field name='code' required="1"/>
                    <field name='name' required="1"/>
                    <field name='beg_date' required="1"/>
                    <field name='end_date' required="1"/>
                    <field name='remark' />
                </tree>
            </field>
        </record>
    </data>
</odoo>